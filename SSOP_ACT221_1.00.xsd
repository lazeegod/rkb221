    <xs:complexType name="CrossBorderLineEqLocation">
        <xs:annotation>
            <xs:documentation>Места подключения своих средств связи к линиям связи, пересекающим государственную границу РФ.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- key-ref -->
            <xs:element name="CrossBorderLineCe" type="CeId" minOccurs="1">
