    <!-- ________________________________________________________________ -->
    <!-- Вводятся форматы данных IpAddr и IpSubnet -->
    <xs:simpleType name="IPv4">
        <xs:annotation>
            <xs:documentation>[0-255].[0-255].[0-255].[0-255], где [0-255] – число от 0 до 255</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="((^|\.)((25[0-5])|(2[0-4]\d)|(1\d\d)|([1-9]?\d))){4}(/([1-2]?[0-9]|3[0-2]))?$" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="IPv6">
        <xs:annotation>
            <xs:documentation>[xxxx]:[xxxx]:[xxxx]:[xxxx]:[xxxx]:[xxxx]:[xxxx]:[xxxx], где [xxxx] – это строка длиной от 0 до 4 символов из набора {0-9, a-f, A-F}</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="((^|:)([0-9a-fA-F]{0,4})){1,8}(/([0-9]|[1-9][0-9]|1[0-1][0-9]|12[0-8]))?$" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="IPv4_subnet">
        <xs:annotation>
            <xs:documentation>[0-255].[0-255].[0-255].[0-255]/[0-32]</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="((^|\.)((25[0-5])|(2[0-4]\d)|(1\d\d)|([1-9]?\d))){4}(/([1-2]?[0-9]|3[0-2]))" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="IPv6_subnet">
        <xs:annotation>
            <xs:documentation>[xxxx]:[xxxx]:[xxxx]:[xxxx]:[xxxx]:[xxxx]:[xxxx]:[xxxx]/[0-128]</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="((^|:)([0-9a-fA-F]{0,4})){1,8}(/([0-9]|[1-9][0-9]|1[0-1][0-9]|12[0-8]))" />
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="IpAddr">
        <xs:annotation>
            <xs:documentation>IP-адрес IPv4/IPv6 или подсеть IPv4/IPv6</xs:documentation>
        </xs:annotation>
        <xs:choice>
            <xs:element name="IPv4" type="IPv4" minOccurs="0" />
            <xs:element name="IPv6" type="IPv6" minOccurs="0" />
            <xs:element name="IPv4_subnet" type="IPv4_subnet" minOccurs="0" />
            <xs:element name="IPv6_subnet" type="IPv6_subnet" minOccurs="0" />
        </xs:choice>
    </xs:complexType>

    <xs:complexType name="IpSubnet">
        <xs:annotation>
            <xs:documentation>Подсеть IPv4/IPv6</xs:documentation>
        </xs:annotation>
        <xs:choice>
            <xs:element name="IPv4_subnet" type="IPv4_subnet" minOccurs="0" />
            <xs:element name="IPv6_subnet" type="IPv6_subnet" minOccurs="0" />
        </xs:choice>
    </xs:complexType>
</xs:schema>