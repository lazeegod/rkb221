            </xs:annotation>
        </xs:element>
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="TrafficExchangePoint">
        <xs:annotation>
            <xs:documentation>Точки обмена трафиком.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="TrafficExchangePointId" type="xs:string" minOccurs="1">
